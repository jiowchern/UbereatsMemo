{
	"manifest_version": 3,
	"name": "UberEats Memo",
	"version": "0.2.0",
	"description": "UberEats Memo is a simple memo tool that adds a memo input box to each item.",
	"author": "jiowchern",
	"permissions": [
		"storage"
	],
	"icons":
	{
		"48": "icon.png",
		"128": "memo_logo_128x128.png"
	},	
	"content_scripts": 
	[
		{
			"matches": ["https://www.ubereats.com/**"],
			"js": ["contentscript.js"]

		}
	],
	"background": {
		"service_worker" :"background.js"		

	  }
}